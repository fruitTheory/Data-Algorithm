{
  "tasks":[
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHs",
        "/nologo",
        "/Fe${cwd}\\debug\\${fileBasenameNoExtension}.exe",
        "${file}",
        "/I", "${cwd}\\src\\Includes",
        "/I", "${cwd}\\src\\DataStructures",
        "/I", "${cwd}\\src\\Algorithms"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher":[
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    },

    {
      "type": "cppbuild",
      "label": "Build with CMake",
      "command": "cmake",
      "args": [
        "--build", "build"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher":[
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}