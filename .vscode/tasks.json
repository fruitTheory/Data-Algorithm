{
  "tasks":[
    {
      "type": "cppbuild",
      "label": "C/C++: cl.exe build active file",
      "command": "cl.exe",
      "args": [
        "/Zi",
        "/EHsc",
        "/nologo",
        "/fsanitize-address",
        "/Fe${cwd}\\debug\\${fileBasenameNoExtension}.exe",
        "${file}",
        "/I", "${cwd}\\src\\Includes",
        "/I", "${cwd}\\src\\DataStructures"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher":[
        "$msCompile"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}